<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.SwitchControlMapper">

	<select id="querySwitchControlByName" resultType="SwitchControl">
		select * from switchcontrol where ifvalid = '1' and name = #{value}
	</select>

</mapper>

 
